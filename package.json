{
  "name": "my-love",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "./scripts/dev-with-cleanup.sh",
    "dev:raw": "vite",
    "build": "tsc -b && vite build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "preview": "vite preview",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:ui": "vitest --ui",
    "test:unit:coverage": "vitest run --coverage",
    "test:smoke": "node scripts/smoke-tests.cjs",
    "test:e2e": "./scripts/test-with-cleanup.sh",
    "test:e2e:raw": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "predeploy": "npm run build && npm run test:smoke",
    "deploy": "gh-pages -d dist",
    "postdeploy": "echo 'Deployment complete! Run: node scripts/post-deploy-check.cjs [YOUR_URL]'",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint:fix": "eslint . --fix && prettier --write ."
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.90.1",
    "dompurify": "^3.3.1",
    "eventsource": "^4.1.0",
    "framer-motion": "^12.27.1",
    "idb": "^8.0.3",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-window": "^2.2.5",
    "react-window-infinite-loader": "^2.0.1",
    "workbox-window": "^7.4.0",
    "zod": "^4.3.5",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.52.0",
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.58.0",
    "@seontechnologies/playwright-utils": "^3.13.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/dompurify": "^3.2.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@types/react-window": "^2.0.0",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.17",
    "autoprefixer": "^10.4.23",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "fake-indexeddb": "^6.2.5",
    "gh-pages": "^6.3.0",
    "globals": "^16.5.0",
    "happy-dom": "^20.3.9",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "rollup-plugin-visualizer": "^6.0.5",
    "supabase": "^2.65.6",
    "tailwindcss": "^4.1.17",
    "tdd-guard-vitest": "^0.1.6",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.53.1",
    "vite": "^7.3.1",
    "vite-plugin-checker": "^0.12.0",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.17"
  },
  "overrides": {
    "glob": "^12.0.0",
    "js-yaml": "^4.1.1",
    "tar": "^7.5.6"
  },
  "browserslist": [
    "defaults and supports es6-module",
    "maintained node versions"
  ]
}
