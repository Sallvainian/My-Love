{
  "name": "my-love",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "./scripts/dev-with-cleanup.sh",
    "dev:raw": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:ui": "vitest --ui",
    "test:unit:coverage": "vitest run --coverage",
    "test:smoke": "node scripts/smoke-tests.cjs",
    "test:e2e": "./scripts/test-with-cleanup.sh",
    "test:e2e:raw": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "predeploy": "npm run build && npm run test:smoke",
    "deploy": "gh-pages -d dist",
    "postdeploy": "echo 'Deployment complete! Run: node scripts/post-deploy-check.cjs [YOUR_URL]'",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint:fix": "eslint . --fix && prettier --write ."
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.84.0",
    "eventsource": "^4.0.0",
    "framer-motion": "^12.23.24",
    "idb": "^8.0.3",
    "lucide-react": "^0.554.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "workbox-window": "^7.3.0",
    "zod": "^4.1.13",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.13",
    "@vitest/ui": "^4.0.15",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.22",
    "fake-indexeddb": "^6.2.5",
    "gh-pages": "^6.3.0",
    "globals": "^16.5.0",
    "happy-dom": "^20.0.10",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.5",
    "supabase": "^2.58.5",
    "tailwindcss": "^3.4.18",
    "tsx": "^4.20.6",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.47.0",
    "vite": "^7.2.2",
    "vite-plugin-pwa": "^1.1.0",
    "vitest": "^4.0.9"
  },
  "overrides": {
    "glob": "^12.0.0"
  }
}
