{
  "name": "my-love",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "./scripts/dev-with-cleanup.sh",
    "dev:raw": "vite",
    "build": "dotenvx run --overload -- bash -c 'tsc -b && vite build'",
    "perf:build": "mkdir -p docs/performance && npm run typecheck && vite build 2>&1 | tee docs/performance/perf-build.log",
    "perf:bundle-report": "npm run perf:build && node scripts/perf-bundle-report.mjs",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src tests scripts --no-error-on-unmatched-pattern --ignore-pattern 'tests/playwright-report/**' --ignore-pattern 'tests/test-results/**' --ignore-pattern 'playwright-report/**' --ignore-pattern 'test-results/**' --ignore-pattern '_bmad-output/**' --ignore-pattern '_bmad/**' --ignore-pattern '.codex/**'",
    "preview": "dotenvx run --overload -- npx vite preview",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:ui": "vitest --ui",
    "test:unit:coverage": "vitest run --coverage",
    "test:smoke": "node scripts/smoke-tests.cjs",
    "test:e2e": "./scripts/test-with-cleanup.sh",
    "test:e2e:raw": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:p0": "playwright test --grep '\\[P0\\]'",
    "test:p1": "playwright test --grep '\\[P0\\]|\\[P1\\]'",
    "test:burn-in": "bash scripts/burn-in.sh",
    "test:db": "supabase test db",
    "test:ci-local": "bash scripts/ci-local.sh",
    "predeploy": "npm run build && npm run test:smoke",
    "deploy": "gh-pages -d dist",
    "postdeploy": "echo 'Deployment complete! Run: node scripts/post-deploy-check.cjs [YOUR_URL]'",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint:fix": "eslint src tests scripts --fix --no-error-on-unmatched-pattern --ignore-pattern 'tests/playwright-report/**' --ignore-pattern 'tests/test-results/**' --ignore-pattern 'playwright-report/**' --ignore-pattern 'test-results/**' --ignore-pattern '_bmad-output/**' --ignore-pattern '_bmad/**' --ignore-pattern '.codex/**' && prettier --write ."
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.95.3",
    "dompurify": "^3.3.1",
    "eventsource": "^4.1.0",
    "framer-motion": "^12.34.0",
    "idb": "^8.0.3",
    "lucide-react": "^0.564.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-window": "^2.2.7",
    "react-window-infinite-loader": "^2.0.1",
    "workbox-window": "^7.4.0",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.1",
    "@currents/playwright": "^1.21.1",
    "@dotenvx/dotenvx": "^1.52.0",
    "@eslint/js": "^9.39.2",
    "@faker-js/faker": "^10.3.0",
    "@playwright/test": "^1.58.2",
    "@seontechnologies/playwright-utils": "^3.13.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/dompurify": "^3.2.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/react-window": "^2.0.0",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.17",
    "autoprefixer": "^10.4.24",
    "dotenv": "^17.3.1",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "fake-indexeddb": "^6.2.5",
    "gh-pages": "^6.3.0",
    "globals": "^17.3.0",
    "happy-dom": "^20.6.1",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "rollup-plugin-visualizer": "^6.0.5",
    "supabase": "^2.76.8",
    "tailwindcss": "^4.1.17",
    "tdd-guard-vitest": "^0.1.6",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.55.0",
    "vite": "^7.3.1",
    "vite-plugin-checker": "^0.12.0",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.17"
  },
  "overrides": {
    "glob": "^12.0.0",
    "js-yaml": "^4.1.1",
    "tar": "^7.5.6"
  },
  "browserslist": [
    "defaults and supports es6-module",
    "maintained node versions"
  ]
}
